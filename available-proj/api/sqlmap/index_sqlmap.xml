<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="available">
    <select id="available.getOrders">
        SELECT
            order_idx,
            title,
            b_time,
            e_time,
            content,
            link,
            send_yn,
            output,
            note,
            com_yn,
            com_date,
            import,
            reg_date
        FROM tb_order
        WHERE 
            delete_yn = 'N'
    </select>
</mapper>